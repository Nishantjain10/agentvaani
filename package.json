{
  "name": "agentvaani",
  "description": "Consent-first voice agent platform for verified insurance workers",
  "author": "AgentVaani Team",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit --pretty",
    "check:deps": "knip",
    "check:i18n": "i18n-check -l src/locales -s en -u src -f next-intl",
    "commit": "commit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "storybook:test": "vitest run --config .storybook/vitest.config.mts",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@arcjet/next": "^1.0.0-beta.10",
    "@clerk/localizations": "^3.32.1",
    "@clerk/nextjs": "^6.36.5",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@logtape/logtape": "^1.3.5",
    "@sentry/nextjs": "^9.47.1",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@types/cheerio": "^1.0.0",
    "@types/multer": "^2.0.0",
    "appwrite": "^20.1.0",
    "cheerio": "^1.1.2",
    "elevenlabs": "^1.59.0",
    "multer": "^2.0.2",
    "next": "^15.5.9",
    "next-intl": "^4.6.1",
    "node-appwrite": "^19.1.0",
    "pdf-parse": "^1.1.4",
    "posthog-js": "^1.312.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@chromatic-com/playwright": "^0.12.8",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@faker-js/faker": "^9.9.0",
    "@lingual/i18n-check": "^0.8.16",
    "@next/bundle-analyzer": "^15.5.9",
    "@next/eslint-plugin-next": "^15.5.9",
    "@playwright/test": "^1.57.0",
    "@spotlightjs/spotlight": "^3.0.2",
    "@storybook/addon-a11y": "^9.1.17",
    "@storybook/addon-docs": "^9.1.17",
    "@storybook/addon-vitest": "^9.1.17",
    "@storybook/nextjs-vite": "^9.1.17",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.4",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "checkly": "^6.9.7",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-storybook": "^9.1.17",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "knip": "^5.78.0",
    "lefthook": "^1.13.6",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "rimraf": "^6.1.2",
    "semantic-release": "^24.2.9",
    "storybook": "^9.1.2",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
